{% extends 'base.html.twig' %}

{% block title %}Constante index{% endblock %}

{% block body %}

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Tension</th>
                <th>Pression</th>
                <th>Pouls</th>
                <th>Temperature</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for constante in constantes %}
            <tr>
                <td>{{ constante.id }}</td>
                <td>{{ constante.tension }}</td>
                <td>{{ constante.pression }}</td>
                <td>{{ constante.pouls }}</td>
                <td>{{ constante.temperature }}</td>
                <td>
                    <a href="{{ path('constante_show', {'id': constante.id}) }}">show</a>
                    <a href="{{ path('constante_edit', {'id': constante.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('constante_new') }}">Create new</a>
{% endblock %}
