{{ form_start(form) }}
<div class="card">
<div class="card-body">
    <div class="row">
        <div class="form-group col-md-6">
            {{ form_row(form.prenom) }}
        </div>
        <div class="form-group col-md-6">
            {{ form_row(form.nom) }}
        </div>
    </div>
    <div class="row">
        <div class="form-group col-md-6">
            {{ form_row(form.email) }}
        </div>
        <div class="form-group col-md-6">
            {{ form_row(form.adresse) }}
        </div>
    </div>
    <div class="row">
        <div class="form-group col-md-4">
            {{ form_row(form.cni) }}
        </div>
        <div class="form-group col-md-4">
            {{ form_row(form.dateNaiss) }}
        </div>
        <div class="form-group col-md-4">
            {{ form_row(form.telephone) }}
        </div>
    </div>
</div>
<div class="card-footer">
    <button class="btn btn-primary pull-right">Enregistrer</button>
</div>
</div>
{{ form_end(form) }}
